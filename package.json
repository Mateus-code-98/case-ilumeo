{
  "name": "case-ilumeo-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "validate-env": "ts-node-dev --transpile-only --ignore-watch node_modules src/shared/scripts/validateEnv.ts",
    "initialize-db": "yarn db:dropAll && yarn db:migrate && ts-node-dev --transpile-only --ignore-watch node_modules src/shared/scripts/seedDataToDB.ts",
    "test": "ts-node-dev --transpile-only --ignore-watch node_modules test.ts",
    "build": "tsc",
    "start": "yarn validate-env && yarn build && node build/server",
    "dev": "yarn validate-env && ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "generateModule": "ts-node-dev --transpile-only --ignore-watch node_modules src/shared/framework/generateModule.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:drop": "npx sequelize-cli db:migrate:undo",
    "db:dropAll": "npx sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "pg": "^8.10.0",
    "sequelize": "^6.29.3",
    "typescript": "^5.0.2",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.3",
    "@types/yup": "^0.32.0",
    "ts-node-dev": "^2.0.0"
  }
}
